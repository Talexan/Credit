<?php
namespace Talexan\Credit\Api\Data;

/**
* Interface for coins history search results.
* @api
* @since 100.*.*
*/
interface LoyaltyCoinsHistorySearchResultInterface extends \Magento\Framework\Api\SearchResultsInterface
{
    /**
    * Get history items list.
    *
    * @return LoyaltyCoinsHistoryInterface[]
    */
    public function getItems();

    /**
    * Set history list.
    *
    * @param LoyaltyCoinsHistoryInterface[] $items
    * @return $this
    */
    public function setItems(array $items);
}
